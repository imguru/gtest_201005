[93m 37 [m    std::string s2 = [95m"hello"[m;
[93m 38 [m[36;1H[K[36;153H38,0-1[9C6%[35;5H[?25h[?25l[1;35r[1;1H[2M[1;36r[34;1H[93m 39 [m    EXPECT_EQ(s1, s2);
